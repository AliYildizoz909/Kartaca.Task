{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "microsoft-signalr@5.0.4",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": [
        "signalr.min.js",
        "signalr.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@5.0.4",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "library": "Chart.js@2.9.4",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "library": "chartjs-adapter-moment@0.1.2",
      "destination": "wwwroot/lib/chartjs-adapter-moment/"
    }
  ]
}